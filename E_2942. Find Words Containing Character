class Solution {
    public List<Integer> findWordsContaining(String[] words, char x) {
     List<Integer> a=new ArrayList<Integer>(words.length);
     for(int i=0;i<words.length;i++){
        String m=words[i];
        for(int j=0;j<m.length();j++){
            if(m.charAt(j)==x){
            a.add(i);
            break;
            }
        }
     }
     return a;

    }
}
